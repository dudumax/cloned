<p id="notice"><%= notice %></p>

<h1>Tweeta</h1>

<table>
  <thead>
    <tr>
      <th>Tweta</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tweeta.each do |tweetum| %>
      <tr>
        <td><%= tweetum.tweta %></td>
        <td><%= link_to 'Show', tweetum %></td>
        <td><%= link_to 'Edit', edit_tweetum_path(tweetum) %></td>
        <td><%= link_to 'Destroy', tweetum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tweetum', new_tweetum_path %>
